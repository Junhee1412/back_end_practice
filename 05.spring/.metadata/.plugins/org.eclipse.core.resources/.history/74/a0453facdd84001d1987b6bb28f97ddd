package com.lec.basic.poly;

public class SonnySpeaker {
	
	public SonnySpeaker() {
		System.out.println("==> Sonny Speaker 객체 생성!");
	}
	
	private void volumnUp() {
		System.out.println("==> Sonny Speaker 볼륨 업!!");
	}
	
	private void volumnDown() {
		System.out.println("==> Sonney Speaker 볼륨 다운!!");
	}
	
}
